{
    "version": 2,
    "public": false,
    "github": {
        "enabled": true,
        "silent": true,
        "autoJobCancelation": true
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        },
        {
            "source": "/api/:match*",
            "destination": "https://chatfood-cdn.s3.eu-central-1.amazonaws.com/fe-code-challenge-1/:match*"
        }
    ]
}
